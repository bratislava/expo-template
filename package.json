{
  "name": "expo-template",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "local-android": "expo start --android --localhost",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "ts:check": "tsc",
    "lint": "eslint --color --ext .md,.ts,.tsx ./",
    "lint:fix": "yarn eslint --fix",
    "doctor": "expo install --check",
    "doctor:fix": "expo install --fix",
    "generate-client-expo-template": "openapi-generator-cli generate -i {URL} -g typescript-axios -o modules/backend/openapi-generated && node ./modules/backend/utils/fix-client.js && prettier --write modules/backend/openapi-generated",
    "generate-clients": "yarn generate-client-expo-template",
    "update-i18next": "yarn add react-i18next@latest i18next@latest -T && yarn add eslint-plugin-i18next@latest -D -T"
  },
  "dependencies": {
    "@types/react": "~18.2.79",
    "axios": "~1.7.7",
    "expo": "~51.0.31",
    "expo-constants": "~16.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-status-bar": "~1.12.1",
    "i18next": "~23.14.0",
    "nativewind": "^4.0.23",
    "react": "18.3.1",
    "react-i18next": "~15.0.1",
    "react-native": "0.74.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "typescript": "~5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.24.5",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-adjunct": "^4.13.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-babel": "^0.2.2",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-auto": "^0.9.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-array-func": "^5.0.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-const-case": "^1.2.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^8.0.0",
    "eslint-plugin-i18next": "~6.0.9",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^4.0.2",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^2.1.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sonarjs": "^0.25.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-xss": "^0.1.12",
    "i18next-parser": "^8.13.0",
    "prettier": "~3.3.3",
    "prettier-plugin-tailwindcss": "~0.6.6",
    "react-native-svg-transformer": "~1.3.0",
    "tailwindcss": "~3.4.10"
  },
  "private": true
}
